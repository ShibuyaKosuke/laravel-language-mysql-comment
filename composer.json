{
    "name": "shibuyakosuke/laravel-language-mysql-comment",
    "license": "MIT",
    "authors": [
        {
            "name": "KosukeShibuya",
            "email": "shibuya@imagenious.co.jp"
        }
    ],
    "autoload": {
        "psr-4": {
            "ShibuyaKosuke\\LaravelLanguageMysqlComment\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ShibuyaKosuke\\LaravelLanguageMysqlComment\\Providers\\CommandServiceProvider",
                "ShibuyaKosuke\\LaravelLanguageMysqlComment\\Providers\\AppServiceProvider"
            ]
        }
    },
    "require": {
        "laravel/laravel": "^7.0"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.5"
    },
    "scripts": {
        "sniffer": [
            "./vendor/bin/phpcs  --standard=phpcs.xml ."
        ],
        "sniffer-rewrite": [
            "./vendor/bin/phpcbf --standard=phpcs.xml ."
        ]
    }
}
